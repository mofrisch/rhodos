---
export interface Props {
  src: string;
  alt: string;
  w: string;
  h: string;
}

const { src, alt, w, h } = Astro.props;
const site = `https://rhodos-weiterstadt.de/cdn-cgi/image/format=auto,width=${w},height=${h}/`;
const csrc = site + src;
---

<img src={csrc} alt={alt} width={w} height={h} />
