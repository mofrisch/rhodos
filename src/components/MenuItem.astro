---
export interface Props {
  item: {
    title: string;
    url: string;
  };
  current: string;
}
const { item, current } = Astro.props;
const pathname = Astro.url.pathname;
---

<li class="flex justify-center hover:text-blue-50">
  <a href={item.url} class={pathname == item.url ? "underline" : ""}>
    {item.title}
  </a>
</li>
