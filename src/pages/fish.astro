---
import Layout from "@layouts/Layout.astro";
import getCategory from "@utils/getCategory";
const title = "Fisch Spezialit√§ten";
const data = await getCategory("Fisch");
---

<Layout>
  <section class="w-max-6xl">
    <h1 class="m-auto py-4 text-center text-3xl">
      {title}
    </h1>
    <div class="w-full">
      <ul>
        {data.map(meal => <li class="w-full">{meal.data.title}</li>)}
      </ul>
    </div>
  </section>
</Layout>
