---
import Meal from "@components/Meal.astro";
import SingleMenu from "@layouts/SingleMenu.astro";
import { getCollection } from "astro:content";
const title = "Vorspeisen";
const coll = "starters";
const soups = await getCollection(coll);
---

<SingleMenu {title}>
  {soups.map(meal => <Meal meal={meal} coll={coll} />)}
</SingleMenu>
