---
import { getCollection } from "astro:content";
import Lunch from "@components/Lunch.astro";
import SingleMenu from "@layouts/SingleMenuLayout.astro";
const title = "Mittagskarte:";
const meals = await getCollection("lunch");
const coll = "lunch";
const current = "/lunch";
---

<SingleMenu {title} {current}>
  {meals.map(meal => <Lunch meal={meal} coll={coll} />)}
</SingleMenu>
